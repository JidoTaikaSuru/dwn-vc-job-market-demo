{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Embedded Wallet React SDK",
  "compilerOptions": {
    "composite": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "allowJs": false,
    "declarationMap": true,
    "declaration": true,
    "jsx": "react-jsx",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "sourceMap": true,
    "target": "ESNext"
  }
}